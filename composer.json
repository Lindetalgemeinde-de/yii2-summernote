{
    "name": "zelenin/yii2-summernote",
    "description": "Yii2 Summernote widget. Super simple WYSIWYG editor on Bootstrap",
    "version": "0.1.0",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "summernote",
        "widget",
        "wysiwyg"
    ],
    "homepage": "https://github.com/zelenin/yii2-summernote-widget",
    "time": "2014-11-09",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Zelenin",
            "email": "aleksandr@zelenin.me",
            "homepage": "http://zelenin.me",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/zelenin/yii2-summernote-widget/issues",
        "source": "https://github.com/germanigortcev/yii2-summernote-widget"
    },
    "require": {
        "yiisoft/yii2": "~2",
        "bower-asset/font-awesome": "~4@stable",
        "bower-asset/codemirror": "@stable",
        "bower-asset/summernote-0.7.3": "0.7.3"
    },
    "autoload": {
        "psr-4": {
            "Zelenin\\yii\\widgets\\Summernote\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm", "bower-asset-library": "vendor/bower"
        }
    }
}
